<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="13" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
        <entry key="HTML" value="true" />
        <entry key="Haml" value="true" />
        <entry key="JSON" value="true" />
        <entry key="JSP" value="true" />
        <entry key="JSPX" value="true" />
        <entry key="Jade" value="true" />
        <entry key="JavaScript" value="true" />
        <entry key="LESS" value="true" />
        <entry key="SASS" value="true" />
        <entry key="SCSS" value="true" />
        <entry key="Stylus" value="true" />
        <entry key="TypeScript" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:PREVIEW" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="2" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="SHOW_INTENTION_BULB" value="false" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="JSEditorOptions">
    <option name="START_STRING_INTERPOLATION_ON_TYPING" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern case-sensitive="true" pattern="\bTODO\b.*" />
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bFIXME\b.*">
      <option name="FOREGROUND" value="b48ead" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="88c0d0" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bTHINK\b.*">
      <option name="FOREGROUND" value="a3be8c" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="88c0d0" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bTODO (LONG-TERM)\b.*">
      <option name="FOREGROUND" value="ebcb8b" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="88c0d0" />
    </pattern>
    <filter name="THINK">
      <pattern index="2" />
    </filter>
    <filter name="TODO">
      <pattern index="3" />
      <pattern index="0" />
    </filter>
    <filter name="FIXME">
      <pattern index="1" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="tagTreeHighlightingOpacity" value="35" />
  </component>
</application>